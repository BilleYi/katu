.contaier{
  position: fixed;
 width: 100%;
 height: 90rpx;
 z-index: 999;
 left: 0;
 top: 0;
 overflow: hidden;
}
.contaierFocus{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 999;
  left: 0;
  top: 0;
background-color: #ccc;
}
.search{
  display: flex;
  align-content: center; 
 z-index: 999;
  margin: 20rpx;
 
}
.search-text{
  display: flex;
  align-items: center;
  flex: 1;
  height: 60rpx;
  border: 1px solid #b4b5b6;
  border-radius: 10rpx;
  background-color: white;
}
.search-text .icon-sousuo{
  margin: 0 10rpx;
}
.search-text input{
  flex: 1;
}
.search-cancel{
  margin: 0 10rpx;
}
.search-history{
  display: flex;
  justify-content: space-between;
  margin: 20rpx;
  margin-bottom: 30rpx;
}
.search-history-btn{
  margin-bottom: 30rpx;
}
.search-history-btn text{
  border: 1px solid #b4b5b6;
  background-color: white;
  border-radius: 20rpx;
  margin: 0 10rpx;
  padding: 10rpx 20rpx;
}
.searchList-item{
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px dashed #bfbfbf;
  padding: 10rpx;
}
.searchList-item view{
  display: flex;
  align-items: center;
}
.searchList-item image{
height: 100rpx;
width: 100rpx;
border-radius: 50%;
}